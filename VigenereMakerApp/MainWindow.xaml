                <Setter Property="FontWeight" Value="SemiBold"></Setter>
                <Setter Property="Command" Value="{Binding Source={StaticResource Locator}, Path=Main.ChangeSelectedCol}"></Setter>
            </Style>
            <Style x:Key="SquareNavCell" TargetType="Button">
